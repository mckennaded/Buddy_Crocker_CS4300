# .env.example

# Django secret key for local development (replace with your own)
SECRET_KEY=your_secret_key_here

# Database URL for local development (replace with your own)
DATABASE_URL=sqlite:///db.sqlite3

# Automated superuser credentials for production deployment
# These must only be set on your production environment (Render dashboard)
# DO NOT include actual values here or in your local environment these are examples
DJANGO_SUPERUSER_USERNAME=admin
DJANGO_SUPERUSER_EMAIL=admin@buddycrocker.com
DJANGO_SUPERUSER_PASSWORD=YourStrongSecurePasswordHere
